<%- include("partial/header") -%>
<h1><%= heading %></h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="exampleInputEmail1">Title</label>
    <input type="text" class="form-control" id="exampleInputEmail1" name="titleBody" aria-describedby="emailHelp"autocomplete="off">
  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Post</label>
    <textarea class="form-control" id="exampleFormControlTextarea1" name ="postBody" rows="3" ></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="list" value=<%= heading %>>Publish</button>
</form>

<%- include("partial/footer") -%>
